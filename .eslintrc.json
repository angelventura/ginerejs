 {
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["warn", 4 ,{ "SwitchCase": 1 }],
        "no-unused-vars": "warn",
        "space-before-function-paren":"off",
        "eol-last":"off",
        "func-names":"off",
        "class-methods-use-this":"off",
        "global-require":"off",
        "no-else-return":"off",
        "prefer-destructuring":"off",
        "arrow-body-style":"off",
        "no-lonely-if":"off"
    }
}
